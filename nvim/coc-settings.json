{
    "languageserver": {
        "bash": {
            "command": "bash-language-server",
            "args": ["start"],
            "filetypes": ["sh"],
            "ignoredRootPaths": ["~"]
        },
        "racket": {
            "command": "racket",
            "args": [
              "--lib",
              "racket-langserver"
            ],
            "filetypes": [
              "scheme"
            ]
        }
    },
    "diagnostic.enable": true,
    "diagnostic.errorSign": "✖",
    "diagnostic.warningSign": "●",
    "diagnostic.infoSign": "●",
    "diagnostic.hintSign": "●",
    "prettier.disableSuccessMessage": true
}
